{
  "name": "mona",
  "description": "A secure foundation for building excellent I18n, PWA & offline-first applications with MUI, Formik & Apollo in Next.js",
  "version": "1.0.0",
  "author": "Mathias WÃ¸bbe",
  "license": "SEE LICENSE IN LICENSE",
  "readme": "README.md",
  "keywords": [
    "next.js",
    "next",
    "material-ui",
    "mui",
    "i18n",
    "i18next",
    "formik",
    "starter",
    "kit",
    "starter-kit",
    "boilerplate",
    "project",
    "tempalte",
    "pwa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MathiasKandelborg/mona-starter-kit.git"
  },
  "main": "server/index.ts",
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=6.5.0"
  },
  "types": "./typescript/*",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "dev": "nodemon server/index.ts && copyfiles static/service-worker.js .next/service-worker.js",
    "start": "NODE_ENV=production node .next/server/production-server/server/index.js",
    "copy": "run-p copy:static copy:settings",
    "copy:static": "copyfiles -u 1 \"static/**/*\" .next/static",
    "copy:settings": "copyfiles -u 1 \"settings/*.js\" .next/server/production-server/settings",
    "build": "next build && tsc --project tsconfig.server.json && npm run copy",
    "build:deploy": "next build && tsc --project tsconfig.server.json && npm run copy:static",
    "analyze-bundle:both": "BUNDLE_ANALYZE=both next build && tsc --project tsconfig.server.json && npx copyfiles -u 1 \"static/**/*\" .next/static",
    "analyze-bundle:server": "BUNDLE_ANALYZE=server next build && tsc --project tsconfig.server.json && npx copyfiles -u 1 \"static/**/*\" .next/static",
    "analyze-bundle:client": "BUNDLE_ANALYZE=browser next build && tsc --project tsconfig.server.json && npx copyfiles -u 1 \"static/**/*\" .next/static",
    "update:graphql": "npm run gen:schema && npm run gen:api-types",
    "gen:schema": "apollo schema:download --endpoint=http://localhost:4000 ./graphql/api.json",
    "gen:api-types": "apollo client:codegen ./typescript/database-api.d.ts --queries=./graphql/**/*.ts --target=typescript --localSchemaFile=./graphql/api.json --outputFlat",
    "lint:js": "prettier-eslint --prettier-last --eslint-config-path .eslintrc.js --write \"{apollo,components,docs,graphql,i18n,pages,server,settings,typescript,util}/**/*.js*\"",
    "lint:ts": "tslint -p tsconfig.json --config tslint.json && prettier-tslint fix \"{apollo,components,docs,graphql,i18n,pages,server,settings,util}/**/*.ts*\"",
    "lint": "npm run lint:ts && npm run lint:js",
    "semantic-release": "semantic-release -e release.config.js",
    "cz": "npx git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@mdi/js": "^3.5.95",
    "@mdi/react": "^1.1.0",
    "apollo": "^2.5.3",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.0",
    "apollo-link": "^1.2.8",
    "apollo-link-context": "^1.0.14",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-link-logger": "^1.2.3",
    "classnames": "^2.2.6",
    "cookie": "^0.3.1",
    "express": "^4.16.4",
    "formik": "^1.5.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "jss": "^9.8.7",
    "kandelborg-date-fns": "^2.0.0-alpha.31",
    "lodash": "^4.17.11",
    "lru-cache": "^5.1.1",
    "mdi-material-ui": "^5.10.0",
    "next": "^8.0.3",
    "next-ga": "^2.3.4",
    "next-i18next": "^0.36.0",
    "numeral": "^2.0.6",
    "react": "^16.8.3",
